    "def is_even(i):\n",
    "    return i%2 == 0\n",
    "zero_odd_list = [1 if is_even(i) else 0 for i in range(100)]\n",
    "out = [f'{i} is {\"even\" if j == 0 else \"odd\"}' for i, j in enumerate(zero_odd_list)]\n",
    "_ = [print(i) for i in out]"
